<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
          <bean class="com.spring.core.collections.Student" name="studcollection">
          <property name="name" value="Viraj"></property>
          <property name="phones">
          	<list>
          		<value>8469904699</value>
          		<value>8469904691</value>
          		<value>8469904693</value>
          	</list>
          </property>
          
          <property name="address">
          	<set>
          		<value>Surat</value>
          		<value>Kutch</value>
          		<value>Baroda</value>
          	</set>
          </property>
          
          <property name="courses">
          <map>
          		<entry key="Java" value="2 Months"></entry>
          		<entry key="Python" value="1 Months"></entry>
          		<entry key="C++" value="1.5 Months"></entry>
          </map>
          </property>
          
          </bean>
           
</beans>

